{
  "name": "@agent-desktop/ccp-admin",
  "version": "1.0.0",
  "description": "Configuration Admin Dashboard for Amazon Connect CCP",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/",
    "deploy:staging": "pnpm run build && ../../infrastructure/scripts/deploy-static.sh staging ccp-admin",
    "deploy:prod": "pnpm run build && ../../infrastructure/scripts/deploy-static.sh production ccp-admin"
  },
  "dependencies": {
    "@agent-desktop/config": "workspace:*",
    "@agent-desktop/logging": "workspace:*",
    "@agent-desktop/types": "workspace:*",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-table": "^8.10.0",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.15.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "keywords": [
    "react",
    "typescript",
    "admin-dashboard",
    "configuration",
    "amazon-connect"
  ],
  "author": "Amazon Connect CCP Team",
  "license": "MIT"
}
