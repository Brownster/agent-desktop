{
  "name": "ccp-config-api-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Amazon Connect CCP Configuration API",
  "bin": {
    "config-api": "bin/config-api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=development",
    "deploy:staging": "cdk deploy --context environment=staging",
    "deploy:prod": "cdk deploy --context environment=production --require-approval=always",
    "diff": "cdk diff",
    "destroy": "cdk destroy",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-cdk": "2.96.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.200.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "lambda",
    "dynamodb",
    "api-gateway",
    "amazon-connect",
    "configuration-api"
  ],
  "author": "Amazon Connect CCP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amazon-connect/ccp-desktop.git",
    "directory": "infrastructure/aws-cdk"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}